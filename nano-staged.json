{
  "*.{js,jsx,ts,tsx}": [
    "npm x -y --package=@biomejs/biome@latest -- biome format --write",
    "npm x -y --package=eslint@latest -- eslint --ext .js,.jsx,.ts,.tsx --config node_modules/@reallyland/tools/.build.eslintrc.json --fix"
  ],
  "package.json": [
    "npm x -y --package=sort-package-json@latest -- sort-package-json"
  ]
}
